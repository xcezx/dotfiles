alias ls='ls -F --color=auto'
alias la='ls -al'
alias ll='ls -lg'

alias sc='ssh-agent-screen'
alias sr='ssh-agent-screen'
alias sl='screen -ls'
alias sx='screen -x'

alias x='exit'

alias g='git'

alias -g L='| less'
alias -g H='| head'
alias -g T='| tail'
alias -g G='| grep'
alias -g W='| wc'

